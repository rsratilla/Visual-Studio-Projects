        <td>
            @Html.DisplayFor(modelItem => item.Clusters.ClusterName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SiteCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sitename)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateDecom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Services)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Reason)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.DecommID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.DecommID })
        </td>
    </tr>
